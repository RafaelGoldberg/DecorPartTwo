@model Tzedakah_Models.OrderHeader

<div class="text-center">
    @if (Model != null)
    {
        <h1 class="text-info">Order Created</h1>
        <p>Order #@Model.Id has been created successfully</p>
    }
    else
    {
        <h1 class="text-info"> Inquiry Confirmation</h1>
    }

    <p>We have received your request. A representative will contact you in the next 24 hours.</p>

    <img src="/Images/patio.jpg" class="w-75" />
</div>

